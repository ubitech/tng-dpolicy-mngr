<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">

  <servers>
    <server>
      <id>nexus-snapshots</id>
      <username>admin</username>
      <password>admin123</password>
    </server>
    <server>
      <id>nexus-releases</id>
      <username>admin</username>
      <password>admin123</password>
    </server>
  </servers>

  <mirrors>
    <mirror>
      <id>central</id>
      <name>central</name>
      <!--<url>http://192.168.1.84:8081/repository/maven-group/</url>-->
      <url>http://son-nexus:8081/repository/maven-group/</url>
      <mirrorOf>*</mirrorOf>
    </mirror>
  </mirrors>
  
<profiles>
    <profile>
      <id>policyProfile</id>
      <repositories>
            <repository>
                <id>maven-snapshots</id>
		<!--<url>http://192.168.1.84:8081/repository/maven-snapshots/</url>-->
		<url>http://son-nexus:8081/repository/maven-snapshots/</url>
		 <releases>
                    <enabled>true</enabled>
                    <updatePolicy>always</updatePolicy>
		 </releases>
                <snapshots>
                    <enabled>true</enabled>
                    <updatePolicy>always</updatePolicy>
                </snapshots>
            </repository>
      </repositories>
     </profile>
   </profiles>

</settings>
